#***************************************************
# configuration file for module aggregations
#***************************************************

# standard modules if no modules are specified on command line
<default>		run cmpl with default modules (used if no modules are specified on command line)
	default

default			run cmpl with default modules
	compatSymbols
	mathFunctions
	inputFunctions
	classFunctions
	externDataFile
    solverSelect
	boundRow
	linearSos -warn-invalid-sos
	linearVarProd
	linearConstantRow
	linearLogCon
	precompile
	compile -no-warn-insert-eos -compat-linenamepref -compat-cbsymbols -compat-cbh-assign
    interpret
    writeMps
    writeMatrix
    solverCbc
    solverGlpk
    solverCplex
    solverGurobi
    solverScip
    solutionReportAscii -solStdOut
    solutionReportCsv
	solutionReportXml



cplex			run cmpl with cplex
	compatSymbols
	mathFunctions
	inputFunctions
	classFunctions
	externDataFile
	boundRow
	linearSos -warn-invalid-sos -native-sos  -native-sos2  
	linearVarProd
	linearConstantRow
	linearLogCon
	precompile
	compile -no-warn-insert-eos -compat-linenamepref -compat-cbsymbols -compat-cbh-assign
    interpret
    writeMps	-mps-format-sos-native
    writeMatrix
    solverCplex -solver cplex
    solutionReportAscii -solStdOut
    solutionReportCsv
	solutionReportXml

gurobi			run cmpl with gurobi
	compatSymbols
	mathFunctions
	inputFunctions
	classFunctions
	externDataFile
	boundRow
	linearSos -warn-invalid-sos -native-sos  -native-sos2  
	linearVarProd
	linearConstantRow
	linearLogCon
	precompile
	compile -no-warn-insert-eos -compat-linenamepref -compat-cbsymbols -compat-cbh-assign
    interpret
    writeMps -mps-format-sos-native
    writeMatrix
    solverGurobi -solver gurobi
    solutionReportAscii -solStdOut
    solutionReportCsv
