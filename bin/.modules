scip			run cmpl with scip
	compatSymbols
	mathFunctions
	inputFunctions
	classFunctions
	externDataFile
	boundRow
	linearSos -warn-invalid-sos -native-sos  -native-sos2  
	linearVarProd
	linearConstantRow
	linearLogCon
	precompile
	compile -no-warn-insert-eos -compat-linenamepref -compat-cbsymbols -compat-cbh-assign
    interpret
    writeMps -mps-format-sos-native
    writeMatrix
    solverScip -solver scip
    solutionReportAscii -solStdOut
